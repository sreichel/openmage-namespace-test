<?xml version="1.0"?>
<config>
    <modules>
        <OpenMage_NamespaceTest>
            <version>1.0.0</version>
        </OpenMage_NamespaceTest>
    </modules>
    <global>
        <blocks>
            <openmage_namespacetest>
                <class>OpenMage\NamespaceTest\Block</class>
            </openmage_namespacetest>
        </blocks>
        <helpers>
            <openmage_namespacetest>
                <class>OpenMage\NamespaceTest\Helper</class>
            </openmage_namespacetest>
        </helpers>
        <models>
            <openmage_namespacetest>
                <class>OpenMage\NamespaceTest\Model</class>
            </openmage_namespacetest>
        </models>
        <resources>
            <openmage_namespacetest_setup>
                <setup>
                    <module>OpenMage\NamespaceTest</module>
                </setup>
            </openmage_namespacetest_setup>
        </resources>
    </global>
    <frontend>
        <routers>
            <openmage_namespacetest>
                <use>standard</use>
                <args>
                    <frontName>namespacetest</frontName>
                    <module>OpenMage\NamespaceTest</module>
                </args>
            </openmage_namespacetest>
        </routers>
    </frontend>
</config>
